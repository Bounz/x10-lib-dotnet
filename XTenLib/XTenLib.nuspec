<?xml version="1.0" encoding="utf-8"?>
<package xmlns="http://schemas.microsoft.com/packaging/2011/08/nuspec.xsd">
    <metadata>
        <id>XTenLib</id>
        <version>1.0.0</version>
        <title>XTenLib</title>
        <authors>Generoso Martello</authors>
        <owners>G-Labs</owners>
